{
  "name": "bun-plugin-dts",
  "version": "0.2.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "A bun plugin for generating dts files",
  "scripts": {
    "build": "bun run build.mjs",
    "prepublishOnly": "bun run build",
    "release": "bumpp && npm publish",
    "lint": "biome check src",
    "format": "biome format src --write"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "bun"
  ],
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/bun-plugin-dts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/bun-plugin-dts.git"
  },
  "bugs": "https://github.com/wobsoriano/bun-plugin-dts/issues",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "dependencies": {
    "dts-bundle-generator": "^9.2.1",
    "get-tsconfig": "^4.7.0"
  },
  "devDependencies": {
    "bumpp": "^9.2.0",
    "bun-types": "^1.0.1",
    "@biomejs/biome": "1.2.2",
    "typescript": "^5.2.2"
  }
}
